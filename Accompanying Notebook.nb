          RowBox[{"-", "1"}]], ")"}], 
        RowBox[{"\[Gamma]", "(", 
         RowBox[{"\[Alpha]", "|"}]}]], 
       SuperscriptBox[
        SubscriptBox["D", "1"], 
        RowBox[{
         RowBox[{
          RowBox[{"ab", "|", "\[Beta]"}], ")"}], "\[Rho]"}]], 
