         RowBox[{"-", "1"}]], ")"}], 
       RowBox[{"\[Gamma]", "(", "\[Alpha]"}]], 
      RowBox[{
       SubscriptBox["\[PartialD]", "t"], 
       SuperscriptBox[
        RowBox[{"(", 
         SuperscriptBox["E", 
          RowBox[{"-", "1"}]], ")"}], 
        RowBox[{
         RowBox[{"\[Beta]", ")"}], "\[Rho]"}]]}]}], "=", 
