Cell[TextData[{
 "We will set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "tit"], "=", 
    SubscriptBox["P", "i"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "dc605cb3-e5cc-4f36-ba18-5c21ab931792"],
 ", ",
 Cell[BoxData[
  FormBox[
